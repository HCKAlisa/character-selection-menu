{
  "name": "tech-test-alisa",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "type-check": "tsc --noEmit",
    "lint": "eslint . --ext .ts",
    "format": "prettier --write \"src/**/*.ts\"",
    "deploy": "npm run build && touch dist/.nojekyll && git add dist -f && git commit -m 'Deploy to GitHub Pages' && git subtree push --prefix dist origin gh-pages"
  },
  "devDependencies": {
    "@types/node": "^24.10.2",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "eslint": "^8.57.1",
    "prettier": "^3.7.4",
    "terser": "^5.44.1",
    "typescript": "~5.9.3",
    "vite": "^7.2.4"
  }
}
